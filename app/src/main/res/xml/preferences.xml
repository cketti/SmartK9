<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
        xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Error -->
    <Preference
            android:key="@string/preference_key_error"
            android:title="@string/preference_status_error" />

    <!-- Clear events -->
    <Preference
            android:key="@string/preference_key_clear"
            android:title="@string/preference_option_clear" />

    <PreferenceCategory android:title="@string/preference_category_other">

        <!-- Email developer -->
        <Preference
                android:key="@string/preference_key_email_developer"
                android:title="@string/preference_option_email_developer_title"
                android:summary="@string/developer_email_address"/>

        <!-- More apps -->
        <Preference
                android:key="@string/preference_key_more_apps"
                android:title="@string/preference_option_more_apps_title"
                android:summary="@string/preference_option_more_apps_summary"/>

    </PreferenceCategory>

</PreferenceScreen>
